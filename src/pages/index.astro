---
import Layout from "../layouts/Layout.astro";

import "./global.css";
---

<Layout title="Welcome to Astro.">
  <main>
    <div class="w-screen h-screen relative">
      <img
        class="object-cover object-center h-full w-full"
        src="/Ebner.jpeg"
        alt=""
      />
      <div class="absolute top-0 left-0 w-full h-full bg-black/25"></div>
      <div class="w-full h-full absolute top-0 left-0 flex">
        <div class="m-auto text-center">
          <br />
          <br />
          <br />
          <br />
          <br />
          <h1 class="text-[10rem] font-mono text-white uppercase">The Rise</h1>
          <p class="text-3xl text-white uppercase font-mono mb-2">
            Regionalna zvijezda u novom odijelu
          </p>
          <h3
            class="text-6xl text-white font-mono"
            style="font-family: hacked;"
          >
            EBNER
          </h3>
          <br />
          <br />
          <br />
          <p class="text-xl text-white font-mono">
            Release date announced soon.
          </p>
          <br />
          <div class="w-full flex">
            <button
              class="mx-auto py-5 px-10 text-white uppercase text-xl"
              style="font-family: hacked; background-image: url(/background-static.png); background-position: center; background-repeat: no-repeat; background-size: cover;"
            >
              Listen now
            </button>
          </div>
        </div>
      </div>
    </div>
    <div class="bg-[#2c2c2c] flex w-full py-10">
      <div class="container mx-auto flex">
        <iframe
          class="mx-auto w-full max-w-xl"
          style="border-radius:12px"
          src="https://open.spotify.com/embed/track/4WEs3KEFu3cYAzCv2Y5BUy?utm_source=generator&theme=0"
          height="152"
          frameborder="0"
          allowfullscreen=""
          allow="autoplay; clipboard-write; encrypted-media; fullscreen; picture-in-picture"
          loading="lazy"></iframe>
      </div>
    </div>
    <div class="w-screen h-[80vh] relative">
      <video
        class="object-cover object-center h-full w-full"
        src="/3d-render-background.mp4"
        autoplay
        loop
      />
      <div class="absolute top-0 left-0 w-full h-full bg-black/25"></div>
      <div class="w-full h-full absolute top-0 left-0 flex">
        <div class="container m-auto text-center">
          <h1 class="text-5xl uppercase font-mono text-white">
            Latest Singles
          </h1>
          <br />
          <br />
          <br />
          <div class="w-full flex items-center justify-center gap-10">
            <div class="w-80 h-80">
              <img
                class="w-full h-full"
                src="data:image/png;base64,"
                alt=""
              />
            </div>
            <div class="w-80 h-80">
              <img
                class="w-full h-full"
                src="https://dertmusic.com/wp-content/uploads/2022/06/rsz_563837cf-8b48-5283-1ba3-e3637031d9a9-1024x1024.jpg"
                alt=""
              />
            </div>
          </div>
          <br />
          <br />
          <button
            class="mx-auto py-5 px-10 bg-white text-black uppercase text-xl shadow"
            style="font-family: hacked;"
          >
            More music
          </button>
        </div>
      </div>
    </div>
    <div class="w-full flex bg-gray-100">
      <div class="container mx-auto p-20">
        <h1 class="text-center text-4xl font-mono uppercase">Shop Music Now</h1>
        <br />
        <br />
        <div class="w-full flex items-center justify-center gap-7">
          <button
            class="bg-black text-white font-mono py-3 px-4 flex items-center justify-center gap-2"
          >
            <img
              src="https://storage.googleapis.com/pr-newsroom-wp/1/2018/11/Spotify_Logo_RGB_White.png"
              class="h-7"
              alt=""
            />
          </button>
          <button
            class="bg-black text-white font-mono py-4 px-5 flex justify-center items-center"
            ><img
              src="https://upload.wikimedia.org/wikipedia/commons/thumb/2/2a/Apple_Music_logo.svg/2560px-Apple_Music_logo.svg.png"
              class="h-5 invert"
              alt=""
            /></button
          >
        </div>
      </div>
    </div>
    <div class="w-full flex bg-gray-200">
      <div class="container mx-auto p-20">
        <h1 class="text-center text-4xl font-mono uppercase">Show Your Love</h1>
        <br />
        <br />
        <div class="w-full flex items-center justify-center gap-7">
          <button
            class="mx-auto py-5 px-10 text-white bg-black uppercase text-xl"
            style="font-family: hacked;"
          >
            Listen now
          </button>
        </div>
      </div>
    </div>
    <div class="w-screen h-[80vh] relative">
      <video
        class="object-cover object-center h-full w-full"
        style="-webkit-filter: grayscale(100%); filter: grayscale(100%);"
        src="/CROWD_JUMPS_LIGHTS_PULSE.mp4"
        autoplay
        loop
        />
      <div class="absolute top-0 left-0 w-full h-full bg-black/25"></div>
      <div class="w-full h-full absolute top-0 left-0 flex">
        <div class="container m-auto">
          <h3 class="font-mono text-white text-3xl text-center">Next event</h3>
          <div class="w-full flex my-3">
            <div class="border-t-2 border-white w-32 mx-auto"></div>
          </div>
          <br />
          <h3 class="font-mono text-white text-4xl text-center">
            Sarajevo, Bosnia & Herzegovina
          </h3>
          <br />
          <br />
          <br />
          <div class="w-full flex justify-center items-start gap-10">
            <div>
              <h1 class="text-9xl font-bold text-white mb-2">00</h1>
              <p class="text-xl text-white uppercase text-center font-bold">
                Days
              </p>
            </div>
            <div>
              <h1 class="text-9xl font-bold text-white mb-2">00</h1>
              <p class="text-xl text-white uppercase text-center font-bold">
                Hours
              </p>
            </div>
            <div>
              <h1 class="text-9xl font-bold text-white mb-2">00</h1>
              <p class="text-xl text-white uppercase text-center font-bold">
                Minutes
              </p>
            </div>
            <div>
              <h1 class="text-9xl font-bold text-white mb-2">00</h1>
              <p class="text-xl text-white uppercase text-center font-bold">
                Seconds
              </p>
            </div>
          </div>
          <br />
          <br />
          <br />
          <div class="w-full flex">
            <button
              class="mx-auto py-5 px-10 text-black bg-white uppercase text-xl"
              style="font-family: hacked;"
            >
              See All Details
            </button>
          </div>
        </div>
      </div>
    </div>
    <div class="w-full flex bg-gray-100">
      <div class="container mx-auto py-20 text-center">
        <h1 class="font-mono text-4xl uppercase">From The Gallery</h1>
      </div>
    </div>
    <div class="w-full flex">
      <div class="container mx-auto flex">
        <div
          class="mx-auto grid grid-cols-4 grid-rows-3 h-[80vh] w-full max-w-6xl"
        >
          <div class="col-span-1 row-span-2 relative">
            <img
              class="object-cover object-center w-full h-full"
              src="/IMG_20221121_173432_414.jpg"
              alt=""
            />
          </div>
          <div class="col-span-1 row-span-1 relative">
            <img
              class="object-cover object-center w-full h-full"
              src="/Screenshot 2021-11-28 at 02.09.24.png"
              alt=""
            />
          </div>
          <div class="col-span-2 row-span-1 relative">
            <img
              class="object-cover object-center w-full h-full"
              src="/IMG_20221128_181003_790.jpg"
              alt=""
            />
          </div>
          <div class="col-span-2 row-span-1 relative">
            <img
              class="object-cover object-top w-full h-full"
              src="/IMG-9ab110ded82189145fe51318676646b7-V.jpg"
              alt=""
            />
          </div>
          <div class="col-span-1 row-span-1 relative">
            <img
              class="object-cover object-center w-full h-full"
              src="/IMG-0888cdbdc7962c8e30857a1b03783719-V.jpg"
              alt=""
            />
          </div>
          <div class="col-span-1 row-span-1 relative">
            <img
              class="object-cover object-center w-full h-full"
              src="/IMG_20221128_181030_073.jpg"
              alt=""
            />
          </div>
          <div class="col-span-1 row-span-1 relative">
            <img
              class="object-cover object-center w-full h-full"
              src="/IMG_20221128_185905_572.jpg"
              alt=""
            />
          </div>
          <div class="col-span-2 row-span-1 relative">
            <img
              class="object-cover object-center w-full h-full"
              src="/Screenshot 2021-11-28 at 01.55.37.png"
              alt=""
            />
          </div>
        </div>
      </div>
    </div>
    <br />
    <br />
    <br />
    <br />
    <br />
    <div class="w-full flex bg-black">
      <div class="container mx-auto py-10 flex items-center">
        <p class="text-white text-xs">
          Copyright © 2023 EBNER All rights reserved - Powered by <a
            href="https://kahvana.com"
            class="underline">Kahvana</a
          >
        </p>
        <a href="https://instagram.com" class="text-white ml-auto"
          ><svg
            xmlns="http://www.w3.org/2000/svg"
            width="20"
            height="20"
            fill="currentColor"
            class="bi bi-instagram"
            viewBox="0 0 16 16"
          >
            <path
              d="M8 0C5.829 0 5.556.01 4.703.048 3.85.088 3.269.222 2.76.42a3.917 3.917 0 0 0-1.417.923A3.927 3.927 0 0 0 .42 2.76C.222 3.268.087 3.85.048 4.7.01 5.555 0 5.827 0 8.001c0 2.172.01 2.444.048 3.297.04.852.174 1.433.372 1.942.205.526.478.972.923 1.417.444.445.89.719 1.416.923.51.198 1.09.333 1.942.372C5.555 15.99 5.827 16 8 16s2.444-.01 3.298-.048c.851-.04 1.434-.174 1.943-.372a3.916 3.916 0 0 0 1.416-.923c.445-.445.718-.891.923-1.417.197-.509.332-1.09.372-1.942C15.99 10.445 16 10.173 16 8s-.01-2.445-.048-3.299c-.04-.851-.175-1.433-.372-1.941a3.926 3.926 0 0 0-.923-1.417A3.911 3.911 0 0 0 13.24.42c-.51-.198-1.092-.333-1.943-.372C10.443.01 10.172 0 7.998 0h.003zm-.717 1.442h.718c2.136 0 2.389.007 3.232.046.78.035 1.204.166 1.486.275.373.145.64.319.92.599.28.28.453.546.598.92.11.281.24.705.275 1.485.039.843.047 1.096.047 3.231s-.008 2.389-.047 3.232c-.035.78-.166 1.203-.275 1.485a2.47 2.47 0 0 1-.599.919c-.28.28-.546.453-.92.598-.28.11-.704.24-1.485.276-.843.038-1.096.047-3.232.047s-2.39-.009-3.233-.047c-.78-.036-1.203-.166-1.485-.276a2.478 2.478 0 0 1-.92-.598 2.48 2.48 0 0 1-.6-.92c-.109-.281-.24-.705-.275-1.485-.038-.843-.046-1.096-.046-3.233 0-2.136.008-2.388.046-3.231.036-.78.166-1.204.276-1.486.145-.373.319-.64.599-.92.28-.28.546-.453.92-.598.282-.11.705-.24 1.485-.276.738-.034 1.024-.044 2.515-.045v.002zm4.988 1.328a.96.96 0 1 0 0 1.92.96.96 0 0 0 0-1.92zm-4.27 1.122a4.109 4.109 0 1 0 0 8.217 4.109 4.109 0 0 0 0-8.217zm0 1.441a2.667 2.667 0 1 1 0 5.334 2.667 2.667 0 0 1 0-5.334z"
            ></path>
          </svg></a
        >
        <a href="https://facebook.com" class="text-white ml-5"
          ><svg
            xmlns="http://www.w3.org/2000/svg"
            width="20"
            height="20"
            fill="currentColor"
            class="bi bi-facebook"
            viewBox="0 0 16 16"
          >
            <path
              d="M16 8.049c0-4.446-3.582-8.05-8-8.05C3.58 0-.002 3.603-.002 8.05c0 4.017 2.926 7.347 6.75 7.951v-5.625h-2.03V8.05H6.75V6.275c0-2.017 1.195-3.131 3.022-3.131.876 0 1.791.157 1.791.157v1.98h-1.009c-.993 0-1.303.621-1.303 1.258v1.51h2.218l-.354 2.326H9.25V16c3.824-.604 6.75-3.934 6.75-7.951z"
            ></path>
          </svg></a
        >
      </div>
    </div>
  </main>
</Layout>
